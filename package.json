{
  "name": "shukran-api",
  "version": "1.0.5",
  "description": "The backend for creators to earn.",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "node server",
    "start": "nodemon index.js --trace-warnings --unhandled-rejections=strict",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config vue-app/config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config vue-app/config/webpack.server.config.js --progress --hide-modules",
    "postinstall": "npm run build"
  },
  "keywords": [
    "creators",
    "earn",
    "audience",
    "Africans"
  ],
  "author": "@wachukxs, @theakomolafe, Obakam Tom-George",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "boom": "^7.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "fastify": "^2.15.3",
    "fastify-cookie": "^5.1.0",
    "fastify-cors": "^3.0.3",
    "fastify-multipart": "1.0.6",
    "fastify-swagger": "^2.6.0",
    "googleapis": "^51.0.0",
    "hoss": "^1.1.1",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "lru-cache": "^5.1.1",
    "money": "^0.2.0",
    "mongoose": "^5.11.12",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.7",
    "route-cache": "^0.4.5",
    "serve-favicon": "^2.5.0",
    "validator": "13.1.17",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
